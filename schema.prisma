datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}
 
model Url {
  id            Int   @id @default(autoincrement())
  createdAt     DateTime @default(now())
  originalURL   String
  shortenedURL  String  @unique
  countClick    Int     @default(0)
}